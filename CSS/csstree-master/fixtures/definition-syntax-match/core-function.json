{
    "a( b* )": {
        "valid": [
            "a()",
            "A()",
            "a(b)",
            "a( b b b )"
        ],
        "invalid": [
            "",
            "a",
            "A",
            "a(",
            "a ()",
            "a (b)",
            "a(x)",
            "a(())",
            "a() foo",
            "a() a()"
        ]
    },
    "[ a( | b( ] c )": {
        "valid": [
            "a(c)",
            "b(c)"
        ],
        "invalid": [
            "",
            "a(b(c))"
        ]
    }
}
