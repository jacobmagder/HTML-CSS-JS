{
    "simple": {
        "syntax": "<bar>",
        "lexer": {
            "types": {
                "bar": "foo"
            }
        },
        "valid": [
            "foo"
        ],
        "invalid": [
            "",
            "bar"
        ]
    },
    "with multiplier": {
        "syntax": "<bar>+",
        "lexer": {
            "types": {
                "bar": "foo"
            }
        },
